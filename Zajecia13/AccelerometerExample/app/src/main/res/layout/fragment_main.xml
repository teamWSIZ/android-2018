<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityFragment"
    tools:showIn="@layout/activity_main">

    <TextView
        android:id="@+id/ax_dsc"
        android:layout_width="78dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/ax_value"
        app:layout_constraintEnd_toStartOf="@+id/ax_value"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/az_value"
        android:layout_width="221dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="260dp"
        android:text="TextView"
        android:textAlignment="viewEnd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ax_value" />

    <TextView
        android:id="@+id/ay_value"
        android:layout_width="221dp"
        android:layout_height="43dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        android:textAlignment="viewEnd"
        app:layout_constraintBottom_toTopOf="@+id/az_value"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ax_value" />

    <TextView
        android:id="@+id/ax_value"
        android:layout_width="221dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="101dp"
        android:text="TextView"
        android:textAlignment="viewEnd"
        app:layout_constraintBottom_toTopOf="@+id/az_value"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ay_dsc"
        android:layout_width="79dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/ay_value"
        app:layout_constraintEnd_toStartOf="@+id/ay_value"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/az_dsc"
        android:layout_width="78dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/az_value"
        app:layout_constraintEnd_toStartOf="@+id/az_value"
        app:layout_constraintStart_toStartOf="parent" />
</android.support.constraint.ConstraintLayout>